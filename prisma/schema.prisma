generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

enum Role{
  ADMIN
  USER
}


model user {
  id    Int     @id @default(autoincrement())
  email String  @unique()
  name  String?
  password String
  role Role @default(USER)
}
